def nthFibonacci(self, n):
        if n==0:
            return 0
        def dp(n,memo={}):
            # code here 
            if n==1 or n==2:
                return 1
            if n not in memo:
                memo[n]=dp(n-1,memo)+dp(n-2,memo)
            return memo[n]
        return dp(n,memo={})
